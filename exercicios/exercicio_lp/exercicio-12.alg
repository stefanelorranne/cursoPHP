// Seção de Declarações 
var
grauf, grauc : real
inicio
// Seção de Comandos 
escreval("Escreva a temperatura em Fahrenheit")
leia(grauf)
grauc := 5 *(grauf-32)/9
escreval(grauf," equivale a ", grauc, " graus celsius")
fimalgoritmo
